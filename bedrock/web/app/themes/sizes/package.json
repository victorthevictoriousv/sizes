{
  "name": "wlsn-npm-build",
  "version": "0.0.1",
  "description": "Run builds for wlsn theme",
  "author": "Samuel Horn <samuel@wilsoncreative.se>",
  "private": true,
  "devDependencies": {
    "autoprefixer": "^9.1.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.26.3",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "imagemin-cli": "^3.0.0",
    "jquery": "^3.3.1",
    "js-cookie": "2.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "onchange": "^4.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "sanitize.css": "^7.0.1",
    "sass-loader": "^7.1.0",
    "svg-spritemap-webpack-plugin": "^2.7.0",
    "svg4everybody": "^2.1.9",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.1",
    "webfontloader": "^1.6.28",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  },
  "scripts": {
    "imagemin": "imagemin --out-dir=dist/img build/img/*.{png,jpg,gif,svg}",
    "watch:img": "onchange 'build/img/*.{png,jpg,gif,svg}' -- npm run imagemin",
    "watch:webpack": "webpack --mode=development --watch",
    "watch": "npm-run-all --parallel watch:*",
    "dev": "webpack --mode=development && npm run imagemin",
    "prod": "webpack --mode=production && npm run imagemin"
  }
}
